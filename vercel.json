{
	"version": 2,
	"builds": [
		{
			"src": "KinoQuizAI/wsgi.py",
			"use": "@vercel/python",
			"config": {
				"maxLambdaSize": "15mb",
				"runtime": "python3.9"
			}
		},
		{
			"src": "build.sh",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "staticfiles"
			}
		}
	],
	"routes": [
		{
			"src": "/static/(.*)",
			"dest": "/static/$1"
        },
		{
			"src": "/(.*)",
			"dest": "KinoQuizAI/wsgi.py"
		}
	],
	"outputDirectory": "staticfiles"
}
